{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 1235",
    "serve": "vite --port 1235 --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "e2e": "npm run e2e:chromium && npm run e2e:firefox",
    "e2e:chromium": "playwright test --project=chromium",
    "e2e:firefox": "playwright test --project=firefox",
    "test:coverage": "vitest run --coverage",
    "test": "vitest run",
    "lint": "eslint --ext .ts,.vue src --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,html,css,scss}\""
  },
  "dependencies": {
    "@asymmetrik/leaflet-d3": "^6.0.1",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/ui": "^3.1.2",
    "@preline/select": "^3.0.0",
    "@tailwindcss/vite": "^4.1.5",
    "@vueuse/core": "^13.2.0",
    "axios": "^1.9.0",
    "d3": "^7.9.0",
    "d3-hexbin": "^0.2.2",
    "file-saver": "^2.0.5",
    "leaflet": "^1.9.4",
    "luxon": "^3.6.1",
    "markdown-it": "^14.1.0",
    "path": "^0.12.7",
    "playwright": "^1.52.0",
    "spin.js": "^4.1.2",
    "tailwindcss": "^4.1.5",
    "universal-cookie": "8.0.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.5",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@iconify-json/heroicons": "^1.2.2",
    "@playwright/test": "^1.52.0",
    "@types/d3": "^7.4.3",
    "@types/d3-hexbin": "^0.2.5",
    "@types/file-saver": "^2.0.7",
    "@types/jsdom": "^21.1.7",
    "@types/leaflet": "^1.9.17",
    "@types/luxon": "^3.6.2",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.15.12",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "3.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "happy-dom": "^17.4.7",
    "jsdoc-to-markdown": "^9.1.1",
    "typedoc-plugin-markdown": "^4.6.3",
    "typedoc-plugin-vue": "^1.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "vue-docgen-cli": "^4.79.0",
    "vue-tsc": "^2.2.8"
  }
}
